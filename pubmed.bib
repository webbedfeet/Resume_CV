	Pmid = {30954970},
	Publisher = {BMJ Publishing Group Ltd},
	Pubmodel = {Print-Electronic},
	Pubstatus = {ppublish},
	Revised = {2019-06-13},
	Title = {Aortic-vertebral interaction in ankylosing spondylitis: syndesmophyte development at the juxta-aortic vertebral rim},
	Volume = {78},
	Year = {2019},
	Bdsk-Url-1 = {https://doi.org/10.1136/annrheumdis-2018-214675}}

