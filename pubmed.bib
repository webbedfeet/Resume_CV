	Owner = {abhijit},
	Pages = {706--10},
	Pmid = {26415107},
	Timestamp = {2016.02.06},
	Title = {Prednisone use and risk of mortality in patients with rheumatoid arthritis: Moderation by use of disease-modifying anti-rheumatic drugs.},
	Volume = {68},
	Year = {2016},
