	Keywords = {Amino Acid Substitution/*genetics, Breast Neoplasms/*classification/*metabolism, Caveolin 1/*genetics/*metabolism, Cell Differentiation, Epithelial Cells/*metabolism, Female, Humans, Immunohistochemistry, Leucine/genetics, Mutation/*genetics, Proline/genetics, Protein Transport},
	Month = {5},
