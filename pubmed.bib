	Issue = {7},
	Journal = {Annals of the Rheumatic Diseases},
	Keywords = {ankylosing spondylitis; aorta; syndesmophyte},
	Month = jul,
	Nlm-Id = {0372355},
	Number = {7},
	Owner = {NLM},
	Pages = {922--928},
